/*  Main code for the screw-propelled mecanum wheels fusion vehicle (ROCO507 module - MCs Robotics - University of Plymouth)    Code to control the four motors */// These constants won't change. They're used to give names to the pins used:const int analogOutPin = 9; // Analog output pin that the Jrk motor controller is attached toint motorSpeed = 0;        // value read from the potint outputValue = 0;        // value output to the PWM (analog out)void setup() {  // initialize serial communications at 9600 bps:  Serial.begin(9600);}void loop() {    Serial.println("Turn on motors");    // set the motor speed - TODO  motorSpeed = 150;  // change the analog out value:  analogWrite(analogOutPin, motorSpeed);  // wait 2 milliseconds before the next loop for the analog-to-digital  // converter to settle after the last reading:  delay(2); }